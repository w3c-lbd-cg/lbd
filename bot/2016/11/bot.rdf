<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:dce="http://purl.org/dc/elements/1.1/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:bot="https://w3id.org/bot#"
    xmlns:dbo="http://dbpedia.org/ontology/"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:cc="http://creativecommons.org/ns#"
    xmlns:vann="http://purl.org/vocab/vann/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#" > 
  <rdf:Description rdf:nodeID="A0">
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
    <rdf:first rdf:resource="https://w3id.org/bot#adjacentElement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/bot#hasElement">
    <owl:propertyChainAxiom rdf:nodeID="A1"/>
    <rdfs:label xml:lang="da">Har bygningsdel</rdfs:label>
    <owl:propertyChainAxiom rdf:nodeID="A2"/>
    <rdfs:comment xml:lang="en">Relation to elements belonging to a building or a storey.</rdfs:comment>
    <rdfs:range rdf:resource="https://w3id.org/bot#Element"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <owl:propertyChainAxiom rdf:nodeID="A3"/>
    <rdfs:label xml:lang="en">Has element</rdfs:label>
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:comment xml:lang="da">Relation til elementer, hørende til en en bygning eller en etage.</rdfs:comment>
    <owl:propertyChainAxiom rdf:nodeID="A4"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/bot#adjacentElement">
    <rdfs:range rdf:resource="https://w3id.org/bot#Element"/>
    <rdfs:domain rdf:resource="https://w3id.org/bot#Space"/>
    <rdfs:comment xml:lang="da">Relation til bygningsdele, som afgrænser et fysisk rum</rdfs:comment>
    <rdfs:comment xml:lang="en">Relation to building elements bounding a physical space</rdfs:comment>
    <rdfs:label xml:lang="da">Tilstødende element</rdfs:label>
    <rdfs:label xml:lang="en">Adjacent element</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A5">
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
    <rdf:first rdf:resource="https://w3id.org/bot#containsElement"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A6">
    <rdf:rest rdf:nodeID="A7"/>
    <rdf:first rdf:resource="https://w3id.org/bot#hasSpace"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A8">
    <owl:members rdf:nodeID="A9"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AllDisjointClasses"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A10">
    <rdf:rest rdf:nodeID="A11"/>
    <rdf:first rdf:resource="https://w3id.org/bot#Space"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/bot">
    <dce:description>A simple ontology describing the topology of a building</dce:description>
    <rdfs:comment xml:lang="da">BygningsTopologiOntologien (BOT) er en simpel ontologi, som definerer kernekoncepterne i en bygning. BOT bør udvides til fagspecifikke formål som for eksempel home automation, indendørs navigation, smart cities mv.</rdfs:comment>
    <dce:creator>Mads Holten Rasmussen (mhoras@byg.dtu.dk)</dce:creator>
    <dce:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#string">November 23rd 2016</dce:modified>
    <rdfs:comment xml:lang="en">The Building Topology Ontology (BOT) is a simple ontology defining the core concepts of a building. BOT should be extended for domain specific purposes such as home automation, indoor navigation, smart cities, etc.</rdfs:comment>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">November 23rd 2016</owl:versionInfo>
    <dce:title>BOT</dce:title>
    <dce:creator>Pieter Pauwels (pipauwel.pauwels@ugent.be)</dce:creator>
    <dce:Language>en</dce:Language>
    <dce:identifier>BOT</dce:identifier>
    <vann:preferredNamespacePrefix>bot</vann:preferredNamespacePrefix>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
    <vann:preferredNamespaceUri>https://w3id.org/bot#</vann:preferredNamespaceUri>
    <cc:license rdf:resource="http://creativecommons.org/licenses/by/3.0/"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/bot#containsElement">
    <rdfs:range rdf:resource="https://w3id.org/bot#Element"/>
    <rdfs:domain rdf:resource="https://w3id.org/bot#Space"/>
    <rdfs:comment xml:lang="da">Relation til en bygningsdel, som er indeholdt i et rum</rdfs:comment>
    <rdfs:comment xml:lang="en">Relation to a building element contained in a space</rdfs:comment>
    <rdfs:label xml:lang="da">Indeholdt i</rdfs:label>
    <rdfs:label xml:lang="en">Contained in</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/bot#name">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label xml:lang="da">Navn</rdfs:label>
    <rdfs:label xml:lang="en">Name</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A12">
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
    <rdf:first rdf:resource="https://w3id.org/bot#adjacentElement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/bot#Building">
    <rdfs:comment xml:lang="da">En uafhængig del af det byggede miljø med en karakteristisk rumlig struktur, der understøtter mindst én funktion eller brugeraktivitet</rdfs:comment>
    <rdfs:comment xml:lang="en">An independent unit of the built environment with a characteristic spatial structure, intended to serve at least one function or user activity [ISO 12006-2:2013]</rdfs:comment>
    <rdfs:label xml:lang="da">Bygning</rdfs:label>
    <rdfs:label xml:lang="en">Building</rdfs:label>
    <owl:sameAs rdf:resource="http://dbpedia.org/ontology/Building"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#class"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A13">
    <rdf:rest rdf:nodeID="A0"/>
    <rdf:first rdf:resource="https://w3id.org/bot#hasSpace"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A1">
    <rdf:rest rdf:nodeID="A5"/>
    <rdf:first rdf:resource="https://w3id.org/bot#hasSpace"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/bot#Element">
    <rdfs:comment xml:lang="da">Bestanddel af et bygværk med en karakteristisk funktion, form eller position</rdfs:comment>
    <rdfs:comment xml:lang="en">Constituent of a construction entity with a characteristic technical function, form or position [12006-2, 3.4.7]</rdfs:comment>
    <rdfs:label xml:lang="da">Bygningsdel</rdfs:label>
    <rdfs:label xml:lang="en">Building element</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#class"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A3">
    <rdf:rest rdf:nodeID="A6"/>
    <rdf:first rdf:resource="https://w3id.org/bot#hasStorey"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A7">
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
    <rdf:first rdf:resource="https://w3id.org/bot#containsElement"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A9">
    <rdf:rest rdf:nodeID="A10"/>
    <rdf:first rdf:resource="https://w3id.org/bot#Element"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A11">
    <rdf:rest rdf:nodeID="A14"/>
    <rdf:first rdf:resource="https://w3id.org/bot#Storey"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/bot#Storey">
    <rdfs:comment xml:lang="da">Et plan i en bygning</rdfs:comment>
    <rdfs:comment xml:lang="en">A level part of a building</rdfs:comment>
    <rdfs:label xml:lang="da">Etage</rdfs:label>
    <rdfs:label xml:lang="en">Storey</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#class"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/bot#hasStorey">
    <rdfs:range rdf:resource="https://w3id.org/bot#Storey"/>
    <rdfs:domain rdf:resource="https://w3id.org/bot#Building"/>
    <rdfs:comment xml:lang="da">Relation til etager indeholdt i bygningen.</rdfs:comment>
    <rdfs:comment xml:lang="en">Relation to storeys contained in the building.</rdfs:comment>
    <rdfs:label xml:lang="da">Har etage</rdfs:label>
    <rdfs:label xml:lang="en">Has storey</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/bot#description">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label xml:lang="da">Beskrivelse</rdfs:label>
    <rdfs:label xml:lang="en">Description</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/bot#Space">
    <rdfs:comment xml:lang="da">En afgrænset tredimensionel udstrækning defineret fysisk eller fiktivt</rdfs:comment>
    <rdfs:comment xml:lang="en">A limited three-dimensional extent defined physically or notionally [ISO 12006-2 (DIS 2013), 3.4.3]</rdfs:comment>
    <rdfs:label xml:lang="da">Rum</rdfs:label>
    <rdfs:label xml:lang="en">Space</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#class"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A14">
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
    <rdf:first rdf:resource="https://w3id.org/bot#Building"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/bot#hasSpace">
    <rdfs:range rdf:resource="https://w3id.org/bot#Space"/>
    <rdfs:domain rdf:resource="https://w3id.org/bot#Storey"/>
    <rdfs:comment xml:lang="da">Relation til rum, hvis plan er placeret på etagen.</rdfs:comment>
    <rdfs:comment xml:lang="en">Relation to spaces whose plan is located at the storey.</rdfs:comment>
    <rdfs:label xml:lang="da">Har rum</rdfs:label>
    <rdfs:label xml:lang="en">Has space</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A4">
    <rdf:rest rdf:nodeID="A12"/>
    <rdf:first rdf:resource="https://w3id.org/bot#hasSpace"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A2">
    <rdf:rest rdf:nodeID="A13"/>
    <rdf:first rdf:resource="https://w3id.org/bot#hasStorey"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/bot#id">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#long"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label xml:lang="da">Id</rdfs:label>
    <rdfs:label xml:lang="en">Id</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
</rdf:RDF>
